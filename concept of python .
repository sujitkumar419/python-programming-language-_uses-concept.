l=[123,45,56,87,90,]
largest=l[0]
index=0

for i in range(len(l)):
    if l[i]>largest:
        largest=l[i]
        index=i

print("your largest number is {largest} at index {index}")

#4.
l=[12,45,67,98]
largest=l[0]

sec_largest=l[0]

for i in l:
    if i>largest:
        sec_largest=largest
        largest=i
    elif i>sec_largest:
        sec_largest=1


print(sec_largest,largest)        

