import turtle

t = turtle.Turtle()
t.speed(2)
t.color("orange")
t.begin_fill()
t.circle(100)  # Diya base
t.end_fill()

t.penup()
t.goto(0, 120)
t.pendown()
t.color("yellow")
t.begin_fill()
t.circle(30)  # Flame
t.end_fill()

# t.hideturtle()
turtle.done()
import turtle

# Setup screen
screen = turtle.Screen()
screen.bgcolor("black")
screen.title("ðŸª” Diwali Diya by Sujit ðŸª”")

# Create turtle
diya = turtle.Turtle()
diya.speed(3)
diya.pensize(3)

# Draw base of diya
diya.penup()
diya.goto(0, -100)
diya.pendown()
diya.color("orange")
diya.begin_fill()
diya.circle(100)
diya.end_fill()

# Draw flame
diya.penup()
diya.goto(0, 50)
diya.pendown()
diya.color("yellow")
diya.begin_fill()
diya.circle(30)
diya.end_fill()

# Add text
diya.penup()
diya.goto(-70, -150)
diya.color("white")
diya.write("Happy Diwali from Mauriyachak!", font=("Arial", 16, "bold"))

# Hide turtle
diya.hideturtle()
turtle.done()






import turtle

# Setup screen
screen = turtle.Screen()
screen.bgcolor("black")
screen.title("ðŸª” Diwali Greeting ðŸª”")

# Create turtle
t = turtle.Turtle()
t.hideturtle()
t.penup()
t.goto(-130, 0)
t.color("white")
t.write("ðŸª” Happy Diwali Sathiyo ðŸª”", font=("Arial", 24, "bold"))

turtle.done()









