class Factory:
    a="pune"
    
    def show(self):
        print("hello i am apune factory")

class Bihar(Factory):
    def show2(self):
        print(super().a)

obj=Bihar()
obj.show2()                

Abstraction
from abc import ABC, abstractmethod

class abstract(ABC):
    @abstractmethod
    def perimeter(self):
        pass

class square(abstract):
    def __init__(self,side):
        self.side=side

class circle(abstract):
    def __init__(self,radius):
        self.radius=radius
    def permeter(self):
        print("i have created")

    def area(self):
        print("I have created this")

obj=circle(7)
                        
Dundermethod #duble square se start and close


class Animal:
    def __str__(self):
        return f"hello how are your name is {self.name}"
    
    def __add__(self,other):
        sum=0
        for i in other:
            sum=sum+i.age

        return f"your sum of ages are {self.age+sum}"

obj=Animal("lion",23)
obj2=Animal("dolphin",45)
obj3=Animal("tiger",56)

print(obj+(obj2+obj3))


#Addvance stuff
class Animal:
    @property
    def show(self):
        print("hello how are you")

obj=Animal()
obj.show        

def decorate(fanc):
    def wrapper(a,b):
        print("I will print myself before the function hello")
        func(a,b)
        print("I will print after the function")
    return wrapper

@decorate
def addition(a,b):
    print("hello I am sujit")

addition(12,45)        

def addition(*args,): #**kwargs
    sum=0
    for i in args:
        sum=sum+i

    print(sum)   
    


