class Animals:
    def show(self):
        print("hello I am sujit")


class Human(Animals):
    def show(self):
        print("how are you")

obj=Human()
obj.show()                

class Animal:
    def show(self):
        print("I am showing")

class Human:
    def show(self):
        print("hello I am also showing")
obj=Animal()
obj2=Human()

obj.show()
obj2.show()


