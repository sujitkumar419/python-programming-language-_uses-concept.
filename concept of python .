class Factory:
    def __init__(self,material,zips,pockets):
        self.material=material
        self.zips=zips
        self.pockets=pockets

    def show(self):
        print(f"your object details are {self.material},{self.pocket},{self.zips}")


reabok=Factory("lleather",3,2)

campus=Factory("nylon",3,3)

reebok.show()

